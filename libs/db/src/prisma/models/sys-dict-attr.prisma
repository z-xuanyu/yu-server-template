model SysDictAttr {
  id     Int     @id @default(autoincrement())
  label  String
  value  String
  remark String  @default("")
  sort   Int     @default(1)
  status Int     @default(1)
  dictId Int
  dict   SysDict @relation(fields: [dictId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  createdAt DateTime         @default(now())
  updatedAt DateTime         @updatedAt

  @@map("sys_dict_attr")
}
