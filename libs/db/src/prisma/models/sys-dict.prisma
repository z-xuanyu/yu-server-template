model SysDict {
  id        Int           @id @default(autoincrement())
  name      String
  code      String
  remark    String        @default("")
  sort      Int           @default(1)
  status    Int           @default(1)
  dictAttrs SysDictAttr[]
  createdAt DateTime      @default(now())
  updatedAt DateTime      @updatedAt

  @@map("sys_dict")
}
