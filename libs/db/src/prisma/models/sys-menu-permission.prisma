model SysMenuPermission {
  menuId     Int
  permissionId Int
  menu       SysMenu @relation(fields: [menuId], references: [id])
  permission SysPermission @relation(fields: [permissionId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  createdAt  DateTime     @default(now())

  @@id([menuId, permissionId])
  @@map("sys_menu_permission")
}