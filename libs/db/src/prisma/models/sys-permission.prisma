model SysPermission {
  id          Int                 @id @default(autoincrement())
  code        String              @unique
  name        String              
  description String
  roles       SysRolePermission[]
  menus       SysMenuPermission[]
  createdAt   DateTime            @default(now())
  updatedAt   DateTime            @updatedAt

  @@map("sys_permission")
}
